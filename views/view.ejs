<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <h2>employee table</h2>
    <p></p>
    
   <%-include('../particals/nav.ejs')%>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>S.NO.</th>
            <th>Name</th>
            <th>city</th>
            <th>Image</th>
            <th>Images</th>
            <th>salary</th>
            <th>Age</th>
            <th>UPDATE</th>
            <th>DELETE</th>
        </tr>
        </thead>
        <tbody>
            <%employeeData.forEach((val,key)=>{%>
                <td><%=key+1%></td>
                <td><%=val.emp_name%></td>
                <td><%=val.city%></td>
                <td><img src="/upload/<%=val.imageName%>" alt="" style="width: 150px;"></td>
                <%val.images.map((val)=>{%>
                    <td><img src="/upload/<%=val.filename%>" alt="" style="width: 150px;"></td>
                <%})%>
                <td><%=val.salary%></td>
                <td><%=val.age%></td>
                <td><a href="/updateemployee/<%=val.id%>"><button>UPDATE</button></a></td>
                <td><a href="/delemployee/<%=val.id%>"><button>DELETE</button></a></td>
            </tr>
            <%})%>
            <!-- employeeData.forEach((val)=>{
                console.log(val.images.map((val)=>{
                    console.log(val.filename)
                }))
            }) -->
            <tr>
           
        </tbody>
    </table>
    
</body>
</html>